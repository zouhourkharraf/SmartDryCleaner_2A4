/*-----QMenu-----*/

QMenu

{

    background-color: #c4c5c3;

    border: 1px solid;

    color: #000000;

	font-weight: bold;



}





QMenu::separator

{

    height: 1px;

    background-color: #363942;

    color: #ffffff;

    padding-left: 4px;

    margin-left: 10px;

    margin-right: 5px;



}





QMenu::item

{

    min-width : 150px;

    padding: 3px 20px 3px 20px;



}





